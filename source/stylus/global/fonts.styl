@font-face
  font-weight: 400
  font-family: "Gotham Pro"
  font-style: normal
  font-display: swap

  src:
    local("Gotham Pro Regular"),
    local("GothamPro-Regular"),
    url("../fonts/gotham-pro--400.woff2") format("woff2"),
    url("../fonts/gotham-pro--400.woff") format("woff")

@font-face
  font-weight: 500
  font-family: "Gotham Pro"
  font-style: normal
  font-display: swap

  src:
    local("Gotham Pro Medium"),
    local("GothamPro-Medium"),
    url("../fonts/gotham-pro--500.woff2") format("woff2"),
    url("../fonts/gotham-pro--500.woff") format("woff")

@font-face
  font-weight: 700
  font-family: "Gotham Pro"
  font-style: normal
  font-display: swap

  src:
    local("Gotham Pro Bold"),
    local("GothamPro-Bold"),
    url("../fonts/gotham-pro--700.woff2") format("woff2"),
    url("../fonts/gotham-pro--700.woff") format("woff")

@font-face
  font-weight: 500
  font-family: "Museo Sans"
  font-style: normal
  font-display: swap

  src:
    local("Museo Sans Cyrl 500"),
    local("MuseoSansCyrl-500"),
    url("../fonts/museo-sans--500.woff2") format("woff2"),
    url("../fonts/museo-sans--500.woff") format("woff")
